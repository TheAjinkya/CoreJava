package com.oops.examples;

class College {

	String name;
	String email;
	int contact;
	int id;

	public void Collge(String name, String email, int contact, int id) {

		this.contact = contact;
		this.name = name;
		this.email = email;
		this.id = id;

	}

	public static void main(String[] args) {

		College c1 = new College();

		c1.name= "DYP";
		c1.contact = 001;
		c1.email = "abc"
	}

}