package com.oops.examples;

class GrandFather {

	public static int income;
	public static String property;

	public void riseUp(int income) {

		int total = 0;

		total += income;
	}

	class Son extends GrandFather {

		int newIncome;
		String newProperty;

		public void newRise(int newIncome) {

			newIncome += income;

		}

	}

}